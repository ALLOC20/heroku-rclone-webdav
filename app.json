{
    "name": "rclone",
    "description": "Deploy rclone on Heroku.",
    "keywords": ["rclone"],
    "env": {
        "rclone": {
            "description": "$HOME/.config/rclone/rclone.conf",
            "value": "",
            "required": true
        },
        "start": {
            "description": "rclone startup parameters(The following is just an example)",
            "value": "gd:up --buffer-size 32M --vfs-cache-mode writes",
            "required": true
        },
        "user": {
            "description": "login user",
            "value": "",
            "required": true
        },
        "pass": {
            "description": "login pass",
            "value": "",
            "required": true
        }
    },
    "stack": "container"
}
